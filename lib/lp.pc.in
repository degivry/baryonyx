prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: lp
Description: A dexi processor
Version: @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@
URL: http://www.vle-project.org
Requires: eigen3
Cflags: -I${includedir}/lp-@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@ -DLP_DLL
Libs: -L${libdir} -llp-@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@ 

